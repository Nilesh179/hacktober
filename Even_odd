#include<iostream>
using namespace std;
int main(){

cout<<"Enter the number : ";
    int n;
    cin>>n;

    if(n%2==0&&n%3==0){
        cout<<"Entered number is Divisible by 2 and 3"<<endl;
    
    }
    else if(n%2==0||n%3==0){
        cout<<"Entered number is Divisible by Any of 2 and 3"<<endl;
    
    }
    else{
        cout<<"ntered number is not Divisible by 2 and 3"<<endl;
    }

    return 0;
}
